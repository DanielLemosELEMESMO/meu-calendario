# Credenciais do Google OAuth 2.0
# Copie este arquivo para .env e preencha com suas pr√≥prias credenciais
# obtidas do Google Cloud Platform.

GOOGLE_CLIENT_ID="SEU_CLIENT_ID_AQUI"
GOOGLE_CLIENT_SECRET="SEU_CLIENT_SECRET_AQUI"

# URL do seu backend. Usado para construir a Redirect URI.
BACKEND_BASE_URL="http://localhost:3001"

# URL do frontend (para redirecionar apos login)
FRONTEND_BASE_URL="http://localhost:5173"

# Conexao com o Supabase/Postgres
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Segredo do JWT de sessao
SESSION_JWT_SECRET="UMA_CHAVE_FORTE_AQUI"

# Defina como true se precisar de SSL para o Postgres
DATABASE_SSL="true"
